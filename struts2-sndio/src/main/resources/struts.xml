<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
   "http://struts.apache.org/dtds/struts-2.5.dtd">
<struts>

	<package name="default" extends="struts-default" namespace="/">
		
		<action name="saisie">
			<result name="success" type="dispatcher">/WEB-INF/jsp/saisie.jsp</result>
		</action>
		
		<action name="enregistrer" class="fr.insee.cnio.formation.struts.action.ValidationAction"
			method="enegistrer">
			<result name="input">/WEB-INF/jsp/saisie.jsp</result>
			<result name="success">/WEB-INF/jsp/resultat.jsp</result>			
		</action>
		
	</package>

</struts>