<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
   "http://struts.apache.org/dtds/struts-2.5.dtd">
<struts>

	<package name="default" extends="struts-default" namespace="/">
		<action name="sessionAfficher" class="fr.insee.cnio.formation.struts.action.SessionAction" method="afficher">
			<result>/WEB-INF/jsp/sessionAfficher.jsp</result>
		</action>
		<action name="sessionSubmit"
			class="fr.insee.cnio.formation.struts.action.SessionAction"
			method="miseEnSession">
			<result>/WEB-INF/jsp/sessionSauvegardee.jsp</result>
		</action>
		
		<action name="sessionFormulairePrerempli"
			class="fr.insee.cnio.formation.struts.action.SessionAction"
			method="preremplirFormulaire">
			<result>/WEB-INF/jsp/sessionFormPrerempli.jsp</result>
		</action>

	</package>

</struts>